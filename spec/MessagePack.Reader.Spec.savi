:class MessagePack.Reader.Spec
  :is Spec
  :const describes: "MessagePack.Reader"

  :it "reads nil (1)"
    data = b"\xc0"
    rd   = MessagePack.Reader.new(data)
    assert: rd.read_nil! <: None
    assert: rd.is_exhausted

  :it "reads bool (1)"
    data = b"\xc2"
    rd   = MessagePack.Reader.new(data)
    assert: rd.read_bool! == False
    assert: rd.is_exhausted

  :it "reads bool (2)"
